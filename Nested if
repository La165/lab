#nested if
Username=input('Enter usernsme')
password=input('Enter Password')
if username=="CSEB":
    if password=="AEC":
        print ("VALID STUDENT")
    else :
        print ("INVALID PASSWORD")
else:
     print("INVALID USERNAME")


